{
	"name": "cortex-debug-remote",
	"displayName": "cortex-debug-remote",
	"description": "Extension to support VSCode remote developent with Cortex-Debug",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onDebugResolve:cortex-debug",
		"onCommand:cortex-debug-remote.helloWorld"
	],
	"main": "./dist/extension.js",
	"publisher": "marus25",
	"license": "MIT",
	"extensionKind": [
		"ui"
	],
	"contributes": {
		"commands": [
			{
				"command": "cortex-debug-remote.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/command-exists": "^1.2.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.63.0",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"@vscode/test-electron": "^2.0.3",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"ts-loader": "^9.2.6",
		"typescript": "^4.5.4",
		"webpack": "^5.76.0",
		"webpack-cli": "^4.9.1"
	},
	"dependencies": {
		"command-exists": "^1.2.9",
		"ip": "^1.1.5",
		"vscode-jsonrpc": "^6.0.0"
	}
}
